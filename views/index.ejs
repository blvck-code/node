<!DOCTYPE html>
<html lang="en">
  <%- include('./partials/head.ejs') %>
  <body>
    <%- include('./partials/nav.ejs') %>
    <div class="blogs vontent">
      <h2>All Blogs</h2>
      <% if (blogs.length > 0) { %> <% blogs.forEach(blog => { %>
      <h3 class="title"><%= blog.title %></h3>
      <p class="title"><%= blog.snippet %></p>
      <% }) %> <% } else { %>
      <p>There are no blogs to display</p>
      <% } %>
    </div>
  </body>
</html>
